{
  "name": "noirforge-vscode",
  "displayName": "NoirForge",
  "description": "NoirForge VS Code extension: run NoirForge flows from VS Code.",
  "version": "0.0.0",
  "publisher": "infantmen-labs",
  "license": "Apache-2.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:noirforge.initTemplate",
    "onCommand:noirforge.flow",
    "onCommand:noirforge.sizes",
    "onCommand:noirforge.computeAnalyze",
    "onCommand:noirforge.openArtifactsDir",
    "onCommand:noirforge.openOutDir"
  ],
  "contributes": {
    "commands": [
      {
        "command": "noirforge.initTemplate",
        "title": "NoirForge: Init Template"
      },
      {
        "command": "noirforge.flow",
        "title": "NoirForge: Flow"
      },
      {
        "command": "noirforge.sizes",
        "title": "NoirForge: Sizes"
      },
      {
        "command": "noirforge.computeAnalyze",
        "title": "NoirForge: Compute Analyze"
      },
      {
        "command": "noirforge.openArtifactsDir",
        "title": "NoirForge: Open Artifacts Dir"
      },
      {
        "command": "noirforge.openOutDir",
        "title": "NoirForge: Open Out Dir"
      }
    ]
  }
}
